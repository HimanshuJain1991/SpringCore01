package com.rays.dic;

public   class Bike {
	String color;
	public void showColor() {
		System.out.println(color);
	}
Engine engine;
public Bike(Engine engine) {
	this.engine=engine;
	System.out.println("I am Constructor");
}
public void startEngine() {
	if(engine==null)
		System.out.println("engine not started");
	else
	engine.start();
}
}
